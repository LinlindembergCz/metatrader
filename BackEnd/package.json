    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/typeorm": "^7.0.0",
    "bcrypt": "^4.0.1",
    "class-validator": "^0.12.2",
    "mysql": "^2.18.1",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "ts-mapper": "^1.2.4",
    "typeorm": "^0.2.29"
