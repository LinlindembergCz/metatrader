  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate --config ./orm.config.json --name",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run --config ./orm.config.json",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert --config ./orm.config.json",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create --config ./orm.config.json --name",
    "schema:log": "ts-node node_modules/typeorm/cli.js schema:log --config ./orm.config.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
